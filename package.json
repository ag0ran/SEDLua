{
  "name": "sedlua",
  "displayName": "SEDLua",
  "description": "SeriousEditor Lua",
  "version": "0.0.1",
  "publisher": "Croteam",
  "repository": {
    "type": "git",
    "url": "https://github.com/ag0ran/SEDLua"
  },
  "engines": {
    "vscode": "^1.38.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:lua"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.showDocumentation",
        "title": "SEDLua: Show Documentation"
      },
      {
        "command": "extension.loadDocumentation",
        "title": "SEDLua: Load Documentation"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "worldScripts",
          "name": "World scripts"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.38.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "typescript": "^3.3.1",
    "tslint": "^5.12.1",
    "vscode-test": "^1.2.0"
  },
  "dependencies": {
    "@types/xml2js": "^0.4.5",
    "path": "^0.12.7",
    "xml2js": "^0.4.22"
  }
}
